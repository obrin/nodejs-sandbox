{
  "name": "js-playground",
  "version": "0.0.0",
  "description": "A project template for authoring and publishing JavaScript libraries",
  "license": "MIT",
  "keywords": [],
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base",
    "rules": {
      "semi": [
        2,
        "never"
      ]
    }
  },
  "dependencies": {
    "@types/node": "^7.0.12",
    "async": "^1.5.2",
    "axios": "^0.11.0",
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.4.1",
    "cockrel": "^1.8.4",
    "cockrel-middleware": "^1.2.0",
    "cockrel-mog": "^1.0.1",
    "country-data": "0.0.31",
    "deep-assign": "^2.0.0",
    "deep-diff": "^0.3.4",
    "falcor": "^0.1.17",
    "falcor-express": "^0.1.2",
    "falcor-router": "^0.4.0",
    "flat": "^2.0.0",
    "glob": "^7.0.3",
    "google-libphonenumber": "^2.0.6",
    "googleapis": "^6.1.0",
    "ht-schema": "^3.2.0",
    "html-entities": "^1.2.0",
    "i18next": "^2.5.1",
    "install": "^0.8.1",
    "js-csp": "^0.5.0",
    "json-refs": "^2.1.5",
    "json-schema-ref-parser": "^3.1.2",
    "json2xlsx": "^0.1.6",
    "lodash": "^4.6.1",
    "lodash.curry": "^4.1.1",
    "lodash.flow": "^3.5.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.2",
    "ncp": "^2.0.0",
    "node-jose": "^0.9.1",
    "nodemon": "^1.11.0",
    "normalizr": "^2.0.1",
    "npm": "^3.10.9",
    "pg": "^6.1.0",
    "pg-format": "^1.0.2",
    "placeholders": "^0.3.0",
    "pointfree-fantasy": "^0.1.3",
    "replaceall": "^0.1.6",
    "typescript": "^2.2.2",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "asynquence": "^0.9.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "deref": "^0.6.4",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "google-auth-library": "^0.9.8",
    "googleapis": "^6.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "query-string": "^4.2.2",
    "rimraf": "^2.5.2",
    "sinon": "^2.0.0-pre",
    "tough-cookie": "^2.3.1"
  },
  "scripts": {
    "build": "tsc -p .",
    "build:watch": "tsc -p . --watch",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "publish": "rimraf lib && babel src --out-dir lib",
    "publish:docs": "easystatic deploy docs --repo kriasoft/babel-starter-kit",
    "start:doc": "easystatic start docs",
    "watch": "nodemon index.js",
    "start": "npm run build && node index.js"
  }
}
